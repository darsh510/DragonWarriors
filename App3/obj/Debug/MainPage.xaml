<Page
    x:Class="App3.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:App3"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">
    <Grid Background="#F5C574" Margin="2,2,-2,-2" >

        <TextBlock x:Name="disptextu" Text="sdfgdfgd" Margin="10,10,1235,712" />
        <Button x:ConnectionId='1' x:Name="kill1" 
			Margin="100,419,0,544" 
			                
			RenderTransformOrigin="1.491,0.842" 
			Height="117" 
			Width="261" FontSize="16" Content="Submit"/>

        <Button x:ConnectionId='2' x:Name="user1" 
			Margin="700,180,0,808" 
			                
			RenderTransformOrigin="1.491,0.842" 
			Height="110" 
			Width="238" FontSize="16" Content="Submit"/>

        <Button x:ConnectionId='3' x:Name="user2" 
			Margin="700,360,0,610" 
			                
			RenderTransformOrigin="1.491,0.842" 
			Height="110" 
			Width="238" FontSize="16" Content="Submit"/>

        <Button x:ConnectionId='4' x:Name="user3" 
        	Margin="700,540,0,415" 
        	                
        	RenderTransformOrigin="1.491,0.842" 
        	Height="110" 
        	Width="238" FontSize="16" Content="Submit"/>
        <Button x:ConnectionId='5' x:Name="user4" 
        	Margin="700,720,0,243" 
        	                
        	RenderTransformOrigin="1.491,0.842" 
        	Height="110" 
        	Width="238" FontSize="16" Content="Submit"/>
        
        <Button x:ConnectionId='6' x:Name="user2_Copy2" 
        	Margin="700,900,0,70" 
        	                
        	RenderTransformOrigin="1.491,0.842" 
        	Height="110" 
        	Width="238" FontSize="16" Content="Submit"/>
        
        <Rectangle x:Name="profile1" Fill="#FF505050" Margin="1420,-2,0,0" Stroke="#19000000">
            <Rectangle.Projection>
                <PlaneProjection CenterOfRotationX="10" CenterOfRotationZ="20"/>
            </Rectangle.Projection>
        </Rectangle>
        <TextBlock x:Name="Firstname" Margin="1552,259,0,0" TextWrapping="Wrap" Text="TextBlock" Height="137" Width="358" FontSize="36" Foreground="#FFD7D0DE" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <TextBlock x:Name="Rating" HorizontalAlignment="Left" Margin="1677,403,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="77" Width="197" FontSize="22"/>
        <TextBlock x:Name="Ranking" HorizontalAlignment="Left" Margin="1677,485,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="83" Width="193" FontSize="22" />
        <!--
        <ScrollViewer Height="200"
                Name="scrlView"
                VerticalAlignment="Top">
           
        -->
        <TextBlock x:Name="Submissions" 
			HorizontalAlignment="Left"
			Margin="1681,568,0,0" 
			TextWrapping="Wrap" 
			Text="TextBlock" 
			VerticalAlignment="Top" 
			Width="193" 
			FontSize="22"/>
        <!--</ScrollViewer>-->
        <TextBlock x:Name="RatingTag" HorizontalAlignment="Left" Margin="1519,401,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="48" Width="79" FontSize="22" Foreground="#FF19E89A">
			<Run Text="Rating"/>
			<LineBreak/>
			<Run/>
        </TextBlock>
        <TextBlock x:Name="currentTag" HorizontalAlignment="Left" Margin="1519,485,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="48" Width="125" FontSize="22" Foreground="#FF19E89A" >
			<Run Text="Current"/>
			<LineBreak/>
			<Run/>
        </TextBlock>
        <TextBlock x:Name="SubmissionsTag" HorizontalAlignment="Left" Margin="1519,568,0,0" TextWrapping="Wrap" VerticalAlignment="Top"  Height="48" Width="125" FontSize="22" Foreground="#FF19E89A" >
			<Run Text="Submissions"/>
			<LineBreak/>
			<Run/>
        </TextBlock>
        <Image x:Name="profImg" 
               HorizontalAlignment="Left" 
               Height="190" 
               Margin="1587,33,0,0" 
               VerticalAlignment="Top" 
               Width="225" 
               Source="Assets/profile.jpg"/>
        <Button x:ConnectionId='7' x:Name="Buttocks"                        Height="154" Margin="88,211,0,403" RenderTransformOrigin="0.5,0.5" Width="418">
            <Button.RenderTransform>
                <CompositeTransform ScaleY="-1" ScaleX="-1"/>
            </Button.RenderTransform> Bus BUs
        </Button>
    </Grid>
</Page>

